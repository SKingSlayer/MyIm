<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace绑定一个dao接口-->
<mapper namespace="com.example.demo.MyData.Dao.TalkRecordListDao">

    <insert id="saveTalkRecord" parameterType="com.example.demo.MyData.Entity.TalkRecord">
        INSERT INTO TalkRecordList(send,recv,myTime,history) VALUES (#{send},#{recv},#{myTime},#{history})
    </insert>

</mapper>